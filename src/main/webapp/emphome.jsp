<%@ taglib uri="jakarta.tags.core" prefix="c" %>
<%@page import="com.klef.jfsd.springboot.model.Employee"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
   <%
     Employee emp = (Employee)session.getAttribute("employee");
      if(emp==null)
      {
        response.sendRedirect("empsessionexpiry");
        return ;
      }
    %>  
    
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>User Home</title>
</head>
<style>
	.container {
	            text-align: center;
	            padding: 50px 20px;
	        }
	        .container h2 {
	            font-size: 32px;
	            color: #333;
	            margin-bottom: 30px;
	        }
	        .domains {
	            display: flex;
	            justify-content: space-around;
	            flex-wrap: wrap;
	        }
	        .domain-box {
	            background-color: #d3b8a1;
	            width: 220px;
	            height: 280px;
	            border-radius: 10px;
	            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
	            text-align: center;
	            padding: 20px;
	            margin: 10px;
	            position: relative;
	            transition: transform 0.3s ease;
	        }
	        .domain-box:hover {
	            transform: scale(1.05);
	        }
	        .domain-box img {
	            width: 100px;
	            height: 100px;
	            margin-bottom: 20px;
	        }
	        .domain-box h3 {
	            font-size: 20px;
	            color: #333;
	            margin-bottom: 10px;
	        }
	        .domain-box p {
	            font-size: 16px;
	            color: #555;
	        }

	        /* Button that appears on hover */
	        .upload-btn {
	            display: none;
	            position: absolute;
	            bottom: 20px;
	            left: 50%;
	            transform: translateX(-50%);
	            background-color: #4c3c4c;
	            color: white;
	            border: none;
	            padding: 10px 15px;
	            border-radius: 5px;
	            cursor: pointer;
	        }
	        .domain-box:hover .upload-btn {
	            display: block;
	        }
</style>
<body>
<%@include file="empnavbar.jsp" %>
<h3 style="text-align:center;">Welcome <%=emp.getName()%></h3>

<!-- Main Container -->
    <div class="container">
        <h2>CHOOSE DOMAIN</h2>
        <p>Choose the domain and share the problem you are facing</p>
        
        <div class="domains">
            <!-- Electricity Section with hover animation -->
            <div class="domain-box">
                <img src="data:image/jpeg;base64," alt="Electricity Icon">
                <h3>Amendments</h3>
                <p>Amendments in Indian Constitution.</p>
                <!-- Button that appears on hover -->
                <form action="amendments.jsp" method="post">
                    <button type="submit" class="upload-btn">Read More</button>
                </form>
            </div>

            <!-- Water Section -->
            <div class="domain-box">
                <img src="data:image/jpeg;base64," alt="Water Icon">
                <h3>Fundamental Rights</h3>
                <p>Fundamental Rights in Indian Constitution.</p>
                <form action="fundamentalrights.jsp" method="post">
                    <button type="submit" class="upload-btn">Read More</button>
                </form>
            </div>

            <!-- Road Section -->
            <div class="domain-box">
                <img src="data:image/jpeg;base64," alt="Road Icon">
                <h3>Fundamental Duties</h3>
                <p>Fundamental Duties in Indian Constitution.</p>
                <form action="fundamentalduties.jsp" method="post">
                    <button type="submit" class="upload-btn">Read More</button>
                </form>
            </div>

      

            <!-- Other Section -->
            <div class="domain-box">
				<img src="data:image/jpeg;base64,",alt="Articles in Indian Constitution">
                <h3>Articles</h3>
                <p>Articles in Indian Constitution.</p>
                <form action="articles.jsp" method="post">
                    <button type="submit" class="upload-btn">Read More</button>
                </form>
            </div>
        </div>
    </div>
</body>
</html>